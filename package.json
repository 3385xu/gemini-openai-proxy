{
  "name": "gemini-openai-proxy",
  "private": true,
  "version": "0.1.0",
  "author": "zuisong",
  "homepage": "https://github.com/zuisong/gemini-openai-proxy",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start:deno": "deno run -A main_deno.ts",
    "start:bun": "bun main_bun.ts",
    "start:node": "npm run build:node && node dist/main_node.js",
    "build": "npm run build:cf_worker && npm run build:deno && npm run build:bun && npm run build:node",
    "build:cf_worker": "npm run _esbuild_ main_cloudflare-workers.ts",
    "build:deno": "npm run _esbuild_ main_deno.ts ",
    "build:node": "npm run _esbuild_ -- --platform=node main_node.ts",
    "build:bun": "npm run _esbuild_ main_bun.ts ",
    "test:deno": "deno test",
    "test:node": " node --import tsx/esm --no-warnings --test ./test/**/*",
    "check": "npx @biomejs/biome check --apply .",
    "_esbuild_": "esbuild --bundle --outdir=dist --target=node18,chrome100 --legal-comments=none --tree-shaking=true --format=esm --out-extension:.js=.mjs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "hono": "^3.12.6"
  },
  "devDependencies": {
    "@hono/node-server": "^1.4.0",
    "@types/bun": "^1.0.2",
    "@types/node": "^20.11.5",
    "esbuild": "^0.19.11"
  }
}
