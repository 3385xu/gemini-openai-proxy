{
  "name": "gemini-openai-proxy",
  "private": true,
  "version": "0.1.0",
  "author": "zuisong",
  "homepage": "https://github.com/zuisong/gemini-openai-proxy",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start:deno": "deno run -A main_deno.ts",
    "start:bun": "bun main_bun.ts",
    "start:node": "node --import tsx/esm main_node.ts",
    "build": "npm run build:cf_worker && npm run build:deno_deploy",
    "build:cf_worker": "npm run _esbuild_ main_cloudflare-workers.ts",
    "build:deno_deploy": "npm run _esbuild_ main_deno.ts ",
    "test:deno": "deno test",
    "test:node": " node --import tsx/esm --no-warnings --test ./test/**/*",
    "check": "deno run -A npm:@biomejs/biome check --apply .",
    "_esbuild_": "esbuild --bundle --outdir=dist --target=es2018"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "hono": "^3.12.6"
  },
  "devDependencies": {
    "@hono/node-server": "^1.4.0",
    "@types/bun": "^1.0.2",
    "@types/node": "^20.11.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.11",
    "openai": "^4.24.7",
    "tsx": "^4.7.0"
  }
}